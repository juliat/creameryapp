<%= simple_form_for @user do |f| %>
    <%= f.input :email, :label => "Email Address" %>
    <%= f.input :password, :label => "Password" %>
    <%= f.input :password_confirmation, :label => "Password Confirmation" %>
    <%= f.association :employee %>
   <%=  f.button :submit, :class => "create" %>
<% end %>
