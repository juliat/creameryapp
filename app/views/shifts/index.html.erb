<h1>All Shifts</h1>
<table>
	<thead>
		<tr>
			<td>Date</td>
			<td>Time</td>
			<td>Employee</td>
			<td>Store</td>
		</tr>
	</thead>
	<tbody>
		<% for shift in @shifts %>
		<tr>
			<td><%= shift.date %></td>
			<td><%= link_to(shift.name, shift_path(shift)) %>
			<td><%= link_to(shift.employee.name, employee_path(shift.employee)) %></td>
			<td><%= link_to(shift.store.name, store_path(shift.store)) %></td>
		</tr>
		<% end %>
	</tbody>
</table>
