<!DOCTYPE html>
<html>
	<head>
	  <title>A&amp;M Creamery</title>
	  <%= stylesheet_link_tag "application"%>
	  <%= javascript_include_tag "jquery", "jquery-ui", "jquery_ujs", "application", "dataTables/jquery.dataTables.min.js", "call-tablesort.js" %> 
	  <%= csrf_meta_tags %>
	</head>
	
	<body>
		<div id="header">
			<%= render :partial => "partials/navigation" %>
		</div>
		<div id = "main-wrapper">
			<div id="main">
				<%= yield %>
			</div>
		</div>
		<div id = "footer-wrapper">
			<%=  render :partial  => "partials/footer" %>
		</div>
	</body>
</html>
